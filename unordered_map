/*

#include <bits/stdc++.h>
#include <unordered_map>
using namespace std;
typedef long long ll;
unordered_map<int,int>m;
int main()
{
    int n;
   scanf("%d",&n);
   int a,b;
    for(int i=0;i<n;i++)
    {

      scanf("%d%d",&a,&b);
        if(a==0)
        {
            m[b]=1;
        }
        else if(a==1)
        {
            if(m.count(b)==1)
            {
                printf("yes\n");
            }
            else
            {
                printf("no\n");
            }
        }
    }
    return 0;
}
